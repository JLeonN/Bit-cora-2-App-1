{
  "name": "bit-cora-2-app",
  "version": "2.6.0",
  "description": "Aplicación móvil para registrar tareas y actividades diarias.",
  "productName": "Bitácora 2",
  "author": "Leonardo Núñez <jorgebike.75@hotmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "generar-apk": "quasar build && npx cap copy android && npx cap open android",
    "postinstall": "quasar prepare",
