    "@capacitor/core": "^7.4.2",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.3",
    "@quasar/extras": "^1.16.4",
    "@tabler/icons-vue": "^3.34.1",
    "@zxing/library": "^0.21.3",
    "docx": "^9.5.1",
    "jsbarcode": "^3.12.1",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.0",
    "xlsx": "^0.18.5"
